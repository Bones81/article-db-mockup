const express = require('express')
const router = express.Router()

router.get('/', (req, res) => {
    res.json({'message': 'Clothing controller set up properly'})
})

router.post('/', (req, res) => {
    res.send('Clothing post route connected')
})

router.put('/', (req, res) => {
    res.send('Clothing update route connected')
})

router.delete('/', (req, res) => {
    res.send('Clothing delete route connected')
})

module.exports = router